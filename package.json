{
  "name": "socio_suite",
  "version": "1.0.0",
  "private": "true",
  "scripts": {
    "dev": "pnpm run -r --parallel --aggregate-output dev",
    "build": "pnpm run -r build",
    "start": "pnpm run -r start",
    "dev:frontend": "pnpm run -r --filter @socio_suite/frontend dev",
    "dev:backend": "pnpm run -r --filter @socio_suite/backend dev",
    "build:frontend": "pnpm run -r --filter @socio_suite/database --filter @socio_suite/utils --filter @socio_suite/frontend build",
    "build:backend": "pnpm run -r --filter @socio_suite/database --filter @socio_suite/utils --filter @socio_suite/backend build",
    "build:database": "pnpm run -r --filter @socio_suite/database build",
    "build:utils": "pnpm run -r --filter @socio_suite/utils build",
    "start:frontend": "pnpm run -r --filter @socio_suite/frontend start",
    "start:backend": "pnpm run -r --filter @socio_suite/backend start",
    "db:gen": "pnpm run -r --filter @socio_suite/database db:gen",
    "db:mig": "pnpm run -r --filter @socio_suite/database db:mig",
    "mig:drop": "pnpm run -r --filter @socio_suite/database mig:drop",
    "db:studio": "pnpm run -r --filter @socio_suite/database db:studio"
  },
  "author": "Md Faizan",
  "license": "ISC",
  "packageManager": "pnpm@10.9.0"
}
